#include <iostream>
#include<string.h>
#include<bits/stdc++.h>
using namespace std;
void reverse(char arr[])
{
	int length=sizeof(arr)/sizeof(arr[0]);
	// int length=arr[].length();
	
	for(int i=0;i<length/2;i++)
	{
		swap(arr[i],arr[length-1-i]);
	}
	for(int i=0;i<length;i++)
	{
		cout<<arr[i];
	}

}
int  main()
{

  char str[] = "hello";  
  reverse(str);
  return 0;
}
